pipeline{
    agent any
	environment{
        PATH = "/opt/maven3/bin:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
			   git credentialsId: 'github', url: 'https://github.com/dbee18/Art/'
            }
        }
		
		stage("maven build"){
		    steps{
                sh "mvn clean package"
                // sh "mv target/*.war target/myweb.war"
			}
		}
        
    }
}
